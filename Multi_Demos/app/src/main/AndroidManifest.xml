<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.makeunion.demo"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".BActivity"/>
        <activity android:name=".CActivity"/>
        <activity android:name=".CurvedMotionActivity"/>
        <activity android:name=".NotificationActivity"/>
        <activity android:name=".CirActivity"/>
        <activity android:name=".RipperActivity"/>
        <activity android:name=".SVGActivity"/>
        <activity android:name=".PathViewActivity"/>
        <activity android:name=".ToolbarActivity"
            android:theme="@style/ToolbarTheme"/>
    </application>

</manifest>
